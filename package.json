    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "~2.1.0",
    "react-native-scalable-image": "^1.0.0",
    "react-native-screens": "~3.0.0",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-styled-text": "^1.0.1",
